<div class="container">


    <h1>{{user.displayName}}</h1>

    <div class="card" style="width: 18rem;">
        <img src={{user.picture}} class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">{{user.email}}</h5>
            <p class="card-text">Profile information can be loaded here.</p>
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>
</div>

<div class="container">
    <h3 class="mt-3">Testing Bringing List of Data from another Table</h3>
    <ul>
        {{#each user.posts}}
        <li>
            {{title}} - {{body}}
        </li>
        {{/each}}
    </ul>

</div>

<div class="container mt-4">

    <h2>User Profile Object</h2>
    <p>{{user.userProfile}}</p>

    <p>{{user.name}}</p>

